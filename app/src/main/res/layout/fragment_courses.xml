<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.CoursesFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/insert_courses_students_button_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="insert"/>

        <Button
            android:id="@+id/get_enrollments_button_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="get enrollment"/>

        <Button
            android:id="@+id/get_data_button_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="getData"/>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewEnrollments"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp" />



    </LinearLayout>



</FrameLayout>